# Create a Postgres database on Vercel: https://vercel.com/postgres
POSTGRES_URL=postgresql://postgresuser:postgrespassword@localhost:5432/postgresdb
POSTGRES_PRISMA_URL=postgresql://postgresuser:postgrespassword@localhost:5432/postgresdb?schema=public
POSTGRES_URL_NON_POOLING=postgresql://postgresuser:postgrespassword@localhost:5432/postgresdb?sslmode=disable

# Generate one here: https://generate-secret.vercel.app/32 (only required for localhost)
AUTH_SECRET=secret
